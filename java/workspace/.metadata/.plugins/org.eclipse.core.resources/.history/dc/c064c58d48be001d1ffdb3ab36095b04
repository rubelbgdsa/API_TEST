package class33;

public class Product {
    int prodId;
    String prodName;
    double prodPrice;
    String prodCode;
    Product(int prodId, String prodName,double prodPrice,String prodCode){
        this.prodId=prodId;
        this.prodName=prodName;
        this.prodPrice=prodPrice;
        this.prodCode=prodCode;
    }
      void setCode (String prodCode){
       this.prodCode = prodCode;
      }
      String getCode() {
      return prodCode;
	    }
	
			
	
}
    

