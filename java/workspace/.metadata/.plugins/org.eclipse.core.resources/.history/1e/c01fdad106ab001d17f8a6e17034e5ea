package class04;

public class mathod_Overloading {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// add two number 3 and 5 than print
		addNumbers(16,5);

		   
		// req: add two number 4 and 6 
		addNumbers(4.6,6);   
		   
		   
		//req add Three numbers 1,4 and 3 
        addNumbers(1,4,3);
		   
		   
	}// main
    
	public static void addNumbers (int m, int n) {
		int result = m+n;
		System.out.println(result);
	}
	public static void addNumbers (int m, int n, int s) {
		int result = m+n+s;
		System.out.println(result);
	}
	  
	public static void addNumbers (double m, int n) {
		     double result = m+n;
			System.out.println(result);
	}
}// class 
