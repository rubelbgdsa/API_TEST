package class04;

public class method {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//req: prrint dallas 5 times 
		printCityName5Times("dallas", 5);
		
		//req: print tampa 5 times 
		printCityName5Times("tampa", 3);
		
		//req: print arizona 5 times
		printCityName5Times("arizona", 2);
		
		//req: print dallas 5 times 
		printCityName5Times("dallas", 2);
		
		//req print tampa 5 times
		printCityName5Times("tampa", 3);
		
		//req: print arizona 5 times
		printCityName5Times("arizona", 5);
		

	}//main
                     //must be camel casing
	public static void printCityName5Times(String city, int n) {
		for (int i=1; i<=n; i++)    //method body
			System.out.println(city);
	}

	
	
	

}
