package class33;

public class Person {
   int id;
   String name;
  private int ssn;

   
   Person(int id, String name, int ssn){
	   this.id = id;
	   this.name = name;
	   this.ssn = ssn;
	   
   }
     void setSsn(int ssn) {
    	 this.ssn=ssn;
     }
     int getSsn() {
    	 return this.ssn;
     }
}
    

