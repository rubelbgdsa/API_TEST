package class4_array;

public class Array {

	public static void main(String[] args) {
//	
        int [][]n;
        n = new int [3][4];
        
        n[0][0] = 2;
        n[0][1] = 7;
        n[0][2] = 4;
        n[0][3] = 9;
   
        n[1][0] = 1;
        n[1][1] = 6;
        n[1][2] = 3;
        n[1][3] = 5;
        
        n[2][0] = 7;
        n[2][1] = 8;
        n[2][2] = 8;
        n[2][3] = 2;
        
        for (int i=0; i<=3; i++);
            System.out.println( n[0][3]);
	}

}
