package class04;

public class mathod_Overloading {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// add two number 3 and 5 than print
		addTwoNumbers(16,5);

		   
		// req: add two number 4 and 6 
		addTwoNumbers(4,6);   
		   
		   
		//req add Three numbers 1,4 and 3 
        addThreeNumbers(1,4,3);
		   
		   
	}// main
    
	public static void addTwoNumbers (int m, int n) {
		int result = m+n;
		System.out.println(result);
	}
	public static void addThreeNumbers (int m, int n, int Y) {
		int result = m+n;
		System.out.println(result);
	}
}// class 
